{
  "name": "@squarecloud/api-types",
  "version": "1.0.0",
  "description": "Typings for Square Cloud API",
  "homepage": "https://github.com/squarecloudofc/api-types#readme",
  "exports": {
    "./v2": {
      "types": "./build/v2.d.ts",
      "require": "./build/v2.js",
      "import": "./build/v2.mjs"
    },
    "./utils": {
      "types": "./build/utils.d.ts",
      "require": "./build/utils.js",
      "import": "./build/utils.mjs"
    },
    "./payloads/v*": {
      "types": "./build/payloads/v*/index.d.ts",
      "require": "./build/payloads/v*/index.js",
      "import": "./build/payloads/v*/index.mjs"
    },
    "./rest/v*": {
      "types": "./build/rest/v*/index.d.ts",
      "require": "./build/rest/v*/index.js",
      "import": "./build/rest/v*/index.mjs"
    }
  },
  "scripts": {
    "build": "npm run lint && tsc",
    "lint": "eslint \"./**.ts\" --fix",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squarecloudofc/api-types.git"
  },
  "keywords": [
    "square",
    "squarecloud",
    "api",
    "squarecloud",
    "api",
    "types"
  ],
  "author": "Jo√£o Tonaco <joaotonaco@squarecloud.app>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/squarecloudofc/api-types/issues"
  },
  "files": [
    "{payloads}/**/*.{js,js.map,d.ts,d.ts.map,mjs}",
    "{utils,v*}.{js,js.map,d.ts,d.ts.map,mjs}"
  ],
  "devDependencies": {
    "@squarecloud/eslint-config": "^0.1.6",
    "@types/node": "^20.8.8",
    "eslint": "^8.52.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "typescript": "^5.2.2"
  }
}
